
run = ["sh", "-c", "mvn spring-boot:run"]
entrypoint = "src/main/java/com/tradepro/TradeProApplication.java"

[env]
REPLIT_DISABLE_PACKAGE_LAYER = "1"

[nix]
channel = "stable-22_11"

[packager]
language = "java"

[languages]
[languages.java]
pattern = "**/*.java"

[languages.java.languageServer]
start = "jdt-language-server"

[deployment]
run = ["sh", "-c", "mvn clean spring-boot:run"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 80
externalPort = 80
